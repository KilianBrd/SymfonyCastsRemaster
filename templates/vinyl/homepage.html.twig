{% extends "base.html.twig" %}

{% block title %}
    {{title}} | {{ parent() }}
{% endblock %}

{% block body %}
    <div class="container mt-5">
        <h1 class="mb-5">{{ title }}</h1>

        <div>
            <h3 class="mb-3">
                Musiques/livres: 
            </h3>
            <ul>
                {% for musique in musiques %}
                <li>
                    <p>
                        {{musique.artiste|upper}} - {{musique.musique}}
                    </p>
                </li>  
                {% endfor %}
            </ul>
        </div>
    </div>
{% endblock %}
